<h1>出勤中社員一覧</h1>
<table class="table table-striped table-bordered" >
   <thead>
     <tr>
       <th style="width: 30%">社員番号</th>   
       <th style="width: 35%">名前</th>  
     </tr>
   </thead> 
   <tbody>
       <tr>
         <% @users.each do |user| %>
         <td class="center"><%= user.employee_number %></td>
         <td class="center"><%= user.name %></td>
       </tr>  
       <% end %>
   </tbody> 
</table>
