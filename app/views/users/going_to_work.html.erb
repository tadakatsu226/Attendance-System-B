<h1>出勤中社員一覧</h1>
<table class="table table-striped table-bordered" >
   <thead>
     <tr>
       <th>社員番号</th>   
       <th>名前</th>  
     </tr>
   </thead> 
   <tbody>
      
   
       <tr>
         
         <% @users.each do |user| %>
      
         <td><%= user.employee_number %></td>
         <td><%= user.name %></td>
       </tr>  
       <% end %>
   </tbody> 
</table>
